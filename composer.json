{
  "minimum-stability": "dev",
  "prefer-stable": true
  "name": "BoburKhanUz/laravel-payme",
  "description": "Laravel Payme",
  "type": "library",
  "authors": [
    {
      "name": "Elbek Khamdullaev",
      "email": "elbek.khamdullaev@gmail.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^12.0|^11.0|^10.0|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Khamdullaevuz\\Payme\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Khamdullaevuz\\Payme\\PaymeServiceProvider"
      ],
      "aliases": {
        "Payme": "Khamdullaevuz\\Payme\\Facades\\Payme"
      }
    }
  }

  "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/BoburKhanUz/laravel-payme.git"
    }
]

  
}
